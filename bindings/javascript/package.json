{
  "name": "satox-sdk",
  "version": "0.9.0",
  "description": "JavaScript bindings for the Satox SDK - blockchain operations, NFT management, and post-quantum cryptography",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node-gyp rebuild",
    "dev": "node-gyp rebuild",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "clean": "rm -rf dist target build",
    "install": "node-gyp rebuild",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "satox",
    "blockchain",
    "cryptocurrency",
    "nft",
    "ipfs",
    "sdk",
    "post-quantum",
    "cryptography",
    "falcon",
    "crystals-kyber"
  ],
  "author": "Satoxcoin Core Developers",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "chai": "^4.3.7",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@babel/plugin-transform-modules-commonjs": "^7.22.0",
    "babel-jest": "^29.0.0",
    "node-gyp": "^9.4.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satoverse/satox-sdk.git",
    "directory": "bindings/javascript"
  },
  "bugs": {
    "url": "https://github.com/satoverse/satox-sdk/issues"
  },
  "homepage": "https://github.com/satoverse/satox-sdk#readme",
  "files": [
    "build/Release/*.node",
    "src/**/*.js",
    "src/**/*.ts",
    "README.md",
    "LICENSE"
  ],
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
